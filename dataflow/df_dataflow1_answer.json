{
	"name": "df_dataflow1_answer",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_Json1",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_Parquet1",
						"type": "DatasetReference"
					},
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "flatten2"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          has_more as boolean,",
				"          items as (answer_id as integer, content_license as string, creation_date as integer, is_accepted as boolean, last_activity_date as integer, last_edit_date as integer, owner as (accept_rate as short, account_id as integer, display_name as string, link as string, profile_image as string, reputation as integer, user_id as integer, user_type as string), question_id as integer, score as short)[],",
				"          quota_max as short,",
				"          quota_remaining as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine') ~> source2",
				"source2 foldDown(unroll(items),",
				"     mapColumn(",
				"          answer_id = items.answer_id,",
				"          content_license = items.content_license,",
				"          creation_date = items.creation_date,",
				"          is_accepted = items.is_accepted,",
				"          last_activity_date = items.last_activity_date,",
				"          last_edit_date = items.last_edit_date,",
				"          owner = items.owner,",
				"          question_id = items.question_id,",
				"          score = items.score",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten2",
				"flatten2 derive({owner.account_id} = owner.account_id,",
				"          {owner.reputation} = owner.reputation,",
				"          {owner.user_id} = owner.user_id,",
				"          {owner.user_type} = owner.user_type,",
				"          {owner.profile_image} = owner.profile_image,",
				"          {owner.display_name} = owner.display_name,",
				"          {owner.link} = owner.link) ~> derivedColumn2",
				"derivedColumn2 select(mapColumn(",
				"          answer_id,",
				"          content_license,",
				"          creation_date,",
				"          is_accepted,",
				"          last_activity_date,",
				"          last_edit_date,",
				"          question_id,",
				"          score,",
				"          account_id = {owner.account_id},",
				"          reputation = {owner.reputation},",
				"          user_id = {owner.user_id},",
				"          user_type = {owner.user_type},",
				"          profile_image = {owner.profile_image},",
				"          display_name = {owner.display_name},",
				"          link = {owner.link}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          YEAR as string,",
				"          MONTH as string,",
				"          SUPPLIER as string,",
				"          {ITEM CODE} as string,",
				"          {ITEM DESCRIPTION} as string,",
				"          {ITEM TYPE} as string,",
				"          {RETAIL SALES} as string,",
				"          {RETAIL TRANSFERS} as string,",
				"          {WAREHOUSE SALES} as string",
				"     ),",
				"     format: 'parquet',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink2"
			]
		}
	}
}