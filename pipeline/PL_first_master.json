					"referenceName": "ls_AzureSqlDatabase1",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "insert into logging.pipelinerun(\r\npipelinerunID ,\r\npipelineName ,\r\npipelineStartTime ,\r\npipelineEndTime ,\r\npipelineStatus \r\n) values(\r\n'@{pipeline().RunId}',\r\n'@{pipeline().Pipeline}',\r\ngetUTCdate(),\r\nnull,\r\n'Starting'\r\n)"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
