					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "update logging.pipelinerun\r\nset pipelineEndTime=getUTCdate(),\r\n    pipelineStatus ='@{activity('load').status}'\r\nwhere   pipelinerunID= '@{pipeline().RunId}'\r\nand  pipelineStatus ='Starting'\r\n"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
